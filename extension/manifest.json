{
    "name": "Cutespam",
    "version": "0.1",
    "description": "Share cute drawings",
    "permissions": [
        "storage", "activeTab", "declarativeContent", "nativeMessaging", "tabs", "contextMenus",
        "https://twitter.com/*"
    ],
    "background": {
        "scripts": ["main.js", "cache.js", "contextmenu.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "import.html"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "cutespam@nightfall.moe",
            "strict_min_version": "50.0"
        }
    },
    "manifest_version": 2,

    // Extension ID: ehcbpfiajfajnopkbkfmkdnonpajclnf
    "key": ""
  }